<project name="Relux" default="export">
    <description>
            Relights chunks
    </description>
	<property name="reluxHome" location="${basedir}"/>

	<property name="jarPath" location="${reluxHome}\\build\\Relux.jar"/>

    <!-- - - - - - - - - - - - - - - - - - 
          target: dep                      
         - - - - - - - - - - - - - - - - - -->
    <target name="export">
    	<echo>"Exporting to ${jarPath}"</echo>
    	<jar destfile="${jarPath}" basedir="${reluxHome}/bin" />
    </target>
	
</project>
